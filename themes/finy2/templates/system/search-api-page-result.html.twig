{#
/**
 * @file
 * Default theme implementation for displaying a single search result coming from Search API page.
 *
 * @ingroup themeable
 */
#}
{{ title_prefix }}
<h3{{ title_attributes }}>
  <a href="{{ url }}">{{ title }}</a>
</h3>
{{ title_suffix }}
{% if snippet %}
  <p>{{ content_attributes }}{{ snippet }}</p>
{% endif %}
<div class="details">
{% if practitioner %}
	by {{ practitioner }}<br/>
{% endif %}
{% if price %}
	Price: {{ price }}
{% endif %}
</div>
